{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "target": "ESNext",
        "lib": ["dom", "dom.iterable", "esnext"],
        "module": "esnext",
        "jsx": "react-jsx",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "noEmit": true,
        "noEmitOnError": true,
        "allowJs": false,
        "esModuleInterop": true,
        "isolatedModules": true,
        "strict": true,
        "alwaysStrict": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "checkJs": false,
        "incremental": false,
        "noUncheckedIndexedAccess": true,
        "noUnusedLocals": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "skipDefaultLibCheck": true,
        "pretty": true,
        "plugins": [
            {
                "name": "next"
            }
        ],
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "include": ["src/**/*", "_unused", ".next/types/**/*.ts", ".next/dev/types/**/*.ts"],
    "exclude": ["node_modules", ".next", "dist", "build", "**/*.spec.ts", "**/*.test.ts"]
}
