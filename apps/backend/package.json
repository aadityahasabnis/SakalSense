{
    "name": "@sakalsense/backend",
    "version": "0.0.66",
    "description": "Express backend for SakalSense",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "dev": "tsx watch --env-file=.env.local src/index.ts",
        "build": "tsup",
        "start": "node dist/index.js",
        "type-check": "tsc -p tsconfig.json --noEmit",
        "lint": "eslint src --ext .ts",
        "format": "prettier --check src",
        "clean": "rm -rf dist .eslintcache"
    },
    "dependencies": {
        "argon2": "^0.44.0",
        "compression": "^1.8.1",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "express": "^5.2.1",
        "helmet": "^7.2.0",
        "jsonwebtoken": "^9.0.3",
        "mongoose": "^9.0.2",
        "redis": "^5.10.0",
        "sakalsense-core": "^1.0.2",
        "zod": "^4.2.1"
    },
    "devDependencies": {
        "@types/compression": "^1.8.1",
        "@types/cookie-parser": "^1.4.10",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/jsonwebtoken": "^9.0.10",
        "tsup": "^8.5.1",
        "tsx": "^4.21.0"
    },
    "keywords": [],
    "author": "Aaditya Hasabnis",
    "license": "ISC",
    "packageManager": "pnpm@10.26.2"
}
