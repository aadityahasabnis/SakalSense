{
    "version": 2,
    "installCommand": "echo 'Pre-built, skipping install'",
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["api/**"]
            }
        }
    ],
    "routes": [{ "src": "/(.*)", "dest": "/api/index.js" }]
}
