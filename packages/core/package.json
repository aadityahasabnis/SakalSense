{
    "name": "@sakalsense/core",
    "version": "0.0.0",
    "description": "Shared TypeScript utilities, types, and constants for SakalSense",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "types": "src/index.ts",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "type-check": "tsc -p tsconfig.json --noEmit",
        "lint": "eslint src --ext .ts",
        "format": "prettier --check src",
        "clean": "rm -rf dist .eslintcache"
    },
    "keywords": [],
    "author": "Aaditya Hasabnis",
    "license": "ISC",
    "packageManager": "pnpm@10.20.0"
}
