{
    "name": "sakalsense-core",
    "version": "1.0.0",
    "description": "Shared TypeScript utilities, types, and constants for SakalSense",
    "private": false,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "type-check": "tsc -p tsconfig.json --noEmit",
        "lint": "eslint src --ext .ts",
        "format": "prettier --check src",
        "clean": "rm -rf dist .eslintcache",
        "prepublishOnly": "pnpm run build"
    },
    "keywords": [
        "sakalsense",
        "typescript",
        "shared",
        "utilities",
        "constants"
    ],
    "author": "Aaditya Hasabnis",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/aadityahasabnis/SakalSense.git",
        "directory": "packages/core"
    },
    "homepage": "https://github.com/aadityahasabnis/SakalSense#readme",
    "packageManager": "pnpm@10.26.2"
}
